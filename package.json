{
  "author": "Ben Gourley",
  "name": "cf-auth-middleware",
  "description": "Authentication middleware for APIs using cf-auth-provider",
  "version": "1.3.0",
  "tags": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:clocklimited/cf-auth-middleware"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "main": "auth.js",
  "scripts": {
    "lint": "jshint . --reporter=./node_modules/jshint-full-path/index.js",
    "pretest": "npm run-script lint",
    "test": "istanbul cover ./node_modules/.bin/_mocha -- -R spec -r should",
    "posttest": "istanbul check-coverage --statements 90 --branches 85 --functions 100 --lines 95 && rm -rf coverage",
    "prepublish": "npm test && npm prune"
  },
  "engines": {
    "node": ">=0.8"
  },
  "dependencies": {
    "cf-signature": "^1.1.1"
  },
  "devDependencies": {
    "express": "^4.13.4",
    "hat": "0.0.3",
    "istanbul": "0",
    "jshint": "2",
    "jshint-full-path": "0",
    "mocha": "1",
    "should": "2",
    "supertest": "~0.8.2"
  }
}
